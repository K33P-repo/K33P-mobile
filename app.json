{
  "expo": {
    "name": "k33p-mobil",
    "slug": "k33p-mobil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/K33P.-small.png",
    "scheme": "k33pmobil",
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "backgroundColor": "#000000"
    },
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/K33P.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212",
      "dark": {
        "backgroundColor": "#121212"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deepurple.k33pmobil"
    },
    "android": {
      "minSdkVersion": 26,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.deepurple.k33pmobil"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      ],
      "react-native-vision-camera",
      "expo-secure-store",
      "expo-video",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow K33P to access your camera",
          "microphonePermission": "Allow K33P to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ece5bfc9-764a-4732-a545-2ee6ec9a15e7"
      }
    }
  }
}
